; .editorconfig
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[**.js]
indent_size = 2

[**.scss]
indent_size = 2

[**.html]
max_char = 78
brace_style = expand

[**.go]
indent_style = tab

[Makefile]
indent_style = tab
